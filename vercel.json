{
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
                },
                {
                    "key": "Pragma",
                    "value": "no-cache"
                },
                {
                    "key": "Expires",
                    "value": "0"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/api/vebapi/:path*",
            "destination": "/api/vebapi/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}